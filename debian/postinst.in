#!/bin/sh -e

# Source debconf library.
. /usr/share/debconf/confmodule

if [ -e /usr/share/exodus/exodus-installer-eden ]
then
	/usr/share/exodus/exodus-installer-eden install @EXODUS_VERSION@
fi

update-desktop-database
xdg-desktop-menu install /usr/share/applications/exodus-eden.desktop --novendor

#DEBHELPER#

