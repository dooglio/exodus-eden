#!/bin/sh -e

# Source debconf library.
. /usr/share/debconf/confmodule

if [ "$1" = "purge" ]
then
    rm -f $HOME/Downloads/exodus-eden-linux-x64-@EXODUS_VERSION@.zip
fi

xdg-desktop-menu uninstall /usr/share/applications/exodus-eden.desktop
update-desktop-database

